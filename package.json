  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "vsh-cli": "workspace:*",
    "@types/node": "^22.10.7",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
